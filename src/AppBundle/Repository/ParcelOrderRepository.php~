<?php

namespace AppBundle\Repository;

use Doctrine\ORM\EntityRepository;
use AppBundle\Model\ParcelOrderInterface;

/**
 * ParcelOrderRepository
 *
 * This class was generated by the Doctrine ORM. Add your own custom
 * repository methods below.
 */
class ParcelOrderRepository extends \Doctrine\ORM\EntityRepository
{
public function delete(ParcelOrderInterface $parcel) 
{
	$em = $this->getEntityManager();
	$em->remove($parcel);
	$em->flush();
}
public function save(ParcelInterface $parcel) 
{
	$em = $this->getEntityManager();
	$em->persist($parcel);
	$em->flush();
}


}
